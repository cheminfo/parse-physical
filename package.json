{
  "name": "properties-parser",
  "version": "0.0.2",
  "description": "Try to parse a string containing physical data like bp, mp",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "cheminfo-build --entry src/index.js --root PropertiesParser",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublishOnly": "rollup -c",
    "test": "npm run test-coverage && npm run eslint",
    "test-coverage": "jest --coverage",
    "test-only": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/properties-parser.git"
  },
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/properties-parser/issues"
  },
  "homepage": "https://github.com/cheminfo/properties-parser#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@types/jest": "^26.0.20",
    "cheminfo-build": "^1.1.9",
    "eslint": "^7.18.0",
    "eslint-config-cheminfo": "^5.2.2",
    "jest": "^26.6.3",
    "jest-matcher-deep-close-to": "^2.0.1",
